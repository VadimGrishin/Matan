   ],
   "source": [
    "# Используем лемму Штольца и оцениваем последовательность 1 / [n!^(1/n) - (n-1)!^(1/(n-1))]\n",
    "n = 1\n",
    "s2 = 1\n",
    "for i in range(10**7-1):\n",
    "    s = s2\n",
    "    n += 1\n",
    "    s2 = n**(1/n) * s**((n-1)/n)\n",
    "    if n%(5*10**5) == 0:\n",
    "        print(1/(s2 - s))\n",
    "print()\n",
    "1/(s2 - s), n\n",
    "    "
